{
  "extends": "@tsconfig/node12/tsconfig.json",
  "include": ["./src/", "./t.ts"],
  "compilerOptions": {
    "module": "commonjs",
    "resolveJsonModule": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./dist",
    "importHelpers": false,
    "noImplicitAny": true,
    "skipLibCheck": true                                 /* Skip type checking all .d.ts files. */
  }
}
